build\$(CONFIG)\inc\config.h: $(DEPS_1)
	@echo ..... [Copy] build\$(CONFIG)\inc\config.h
	if not exist "build\$(CONFIG)\inc" md "build\$(CONFIG)\inc"
	copy /Y /B src\mbedtls\config.h+modified build\$(CONFIG)\inc\config.h $(LOG)

